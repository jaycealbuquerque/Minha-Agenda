<div class="container">
	<table class="table">
	  

	  <thead>
	    <tr>
	      <th>Nome</th>
	      <th>Telefone</th>
	      <th>Tipo</th>
	      <th colspan="3"></th>
	    </tr>
	  </thead>

	  <tbody>
	    <% @dash.each do |contato| %>
	      <tr>
	        <td><%= contato.nome %></td>
	        <td><%= contato.telefone %></td>
	        <td><%= contato.tipo.nome %></td>
	        <td><%= link_to 'Mostar', contato%></td>
			<td><%= link_to 'Editar', edit_contato_path(contato) %></td>
	       
	      </tr>
	 	<% end %>
	  </tbody>
	</table>
	<%= paginate @dash %>

	   <section class="busca">
        <% if @busca.present? %>
          <% @busca.each do |serie| %>
            <div class="col-md-12">
              <h1><%= serie.nome %></h1>
              
              
            
            </div>
          <% end %>
        <% end %>
      </selection>
</div>



